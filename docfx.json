{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "Src/Engine/**.csproj"
          ]
        }
      ],
      "dest": "References",
      "filter": "filter.yml",
      "disableGitFeatures": false,
      "disableDefaultFilter": true
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "toc.yml",
          "index.md",
          "References/**.yml",
          "References/**.md",
          "Articles/**.yml",
          "Articles/**.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "Images/**",
          "Logo.png",
          "favicon.ico"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "Overwrite/**.md"
        ],
        "exclude": [
          "obj/**",
          "Site/**"
        ]
      }
    ],
    "globalMetadata": {
      "_appFooter": "Copyright (c) Altseed 2020.",
      "_disableContribution": true,
      "_appTitle": "Altseed2",
      "_enableSearch": true,
      "_appFaviconPath":"favicon.ico"
    },
    "dest": "Site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "mathew/src",
      "TemplateCustomize/"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}